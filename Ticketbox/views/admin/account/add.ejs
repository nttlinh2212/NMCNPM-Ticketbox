<% include ../head %>

    <body id="page-top">
        <!-- Page Wrapper -->
        <div id="wrapper">

            <!-- Sidebar -->
            <% include ../navbar %>
                <!-- End of Sidebar -->

                <!-- Content Wrapper -->
                <div id="content-wrapper" class="d-flex flex-column">

                    <!-- Main Content -->
                    <div id="content">

                        <!-- Header -->
                        <% include ../header %>
                            <!-- End of Header -->

                            <!-- Begin Page Content -->
                            <div class="container-fluid">

                                <!-- Show Data -->
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h1 class="m-0 font-weight-bold text-primary text-center">Add User</h6>
                                    </div>

                                    <div class="card-body">
                                        <div class="container">
                                            <form action="/admin/account/add" method="post">
                                                <div class="form-group">
                                                    <label for="">Id</label>
                                                    <input type="text" name="id" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Name</label>
                                                    <input type="text" name="fullname" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Dob</label>
                                                    <input type="text" name="dob" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Username</label>
                                                    <input type="text" name="username" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Password</label>
                                                    <input type="text" name="password" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Phone</label>
                                                    <input type="text" name="phone" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Point</label>
                                                    <input type="text" name="point" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Favourite genre</label>
                                                    <input type="text" name="favouriteGenre" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                    <label for="">Is admin</label>
                                                    <input type="text" name="isadmin" id="" class="form-control"
                                                        aria-describedby="helpId">
                                                </div>
                                                <button type="submit" class="btn btn-primary" style="width: 100%;">Add
                                                    User</button>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- End of Main Content -->

                            <!-- Footer -->
                            <% include ../footer%>
                                <!-- End of Footer -->
                    </div>
                    <!-- End of Content Wrapper -->
                </div>
                <!-- End of Page Wrapper -->

                <!-- Scroll to Top Button-->
                <a class="scroll-to-top rounded" href="#page-top">
                    <i class="fas fa-angle-up"></i>
                </a>

                <!-- Logout Modal-->
                <% include ../logout %>

                    <!-- Add Javascript-->
                    <% include ../javascripts-addon %>
    </body>

    </html>